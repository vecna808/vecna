END

IF
	Range(LastSeenBy(Myself),5)
	!HasItem("BLAKBLAD",Myself) // Black Blade of Disaster
	!HasItem("PHANBLAD",Myself) // Phantom Blade
	!CheckStatLT(Myself,10,THAC0)
	!CheckStatLT(Myself,0,ARMORCLASS)
	HaveAnySpells()
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
		RunAwayFrom(LastSeenBy(Myself),45)
		SetInterrupt(TRUE)
END


